@model StartspelerMVC.Areas.Admin.Models.Export.ExportModel
@{
    ViewData["Title"] = "Export";
}

<div class="display-1">
    @Html.DisplayFor(m => m.Errors)
</div>
<form asp-area="Admin" asp-controller="Export" asp-action="Download" method="post">

    @foreach (var item in Model.ExportItems)
    {
        <div class="form-check">
            <input class="form-check-input" type="checkbox" value="@item.isSelected" id="flexCheckDefault">
            <label class="form-check-label" for="flexCheckDefault">
                @item.Naam
            </label>
        </div>
    }
    <input type="submit" value="Download" asp-action="Download" class"btn btn-default" />

</form>



